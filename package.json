{
  "name": "stax-utilities",
  "private": false,
  "version": "0.0.0",
  "description": "Project to group common utilities into a single project.",
  "main": "app/main.cjs",
  "author": "Guilherme Bohnstedt",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=develop npm run electron:start & npm run svelte:dev",
    "electron:start": "electron .",
    "electron:make": "npm run make",
    "electron:package": "npm run package",
    "svelte:dev": "vite",
    "svelte:build": "vite build",
    "svelte:preview": "vite preview",
    "svelte:check": "svelte-check --tsconfig ./tsconfig.json",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@iconify-json/ri": "^1.1.3",
    "@popperjs/core": "^2.11.6",
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.10",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "electron": "^20.1.4",
    "flowbite": "^1.5.3",
    "flowbite-svelte": "^0.27.5",
    "postcss": "^8.4.16",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "unplugin-icons": "^0.14.11",
    "vite": "^3.1.0",
    "vite-plugin-dynamic-import": "^1.2.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "kdbxweb": "^2.1.1",
    "svelte-grid": "^5.1.1",
    "svelte-loadable": "^2.0.1",
    "svelte-portal": "^2.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "stax_utilities"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
